{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "http-service": {
      "description": "Creates a httpService.",
      "factory": "./http-service/index#httpService",
      "schema": "./http-service/schema.json"
    },
    "font": {
      "description": "Adds Lato font to index.html.",
      "factory": "./font/index#font"
    },
    "ui-framework": {
      "description": "Adds framework to package.json and install after.",
      "factory": "./ui-framework/index#uiFramework",
      "schema": "./ui-framework/schema.json"
    },
    "list-component": {
      "description": "Creates a component with list.",
      "factory": "./list-component/index#listComponent",
      "schema": "./list-component/schema.json"
    }
  }
}
